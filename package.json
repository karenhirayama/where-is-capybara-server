{
  "name": "photo-tagging-backend",
  "version": "1.0.0",
  "description": "Backend for photo tagging game",
  "main": "dist/server.ts",
  "scripts": {
    "build": "tsc",
    "start": "ts-node src/server.ts",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "migrate": "ts-node scripts/migrate.ts",
    "migrate:dev": "ts-node scripts/migrate.ts"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.3",
    "express-session": "^1.18.2",
    "helmet": "^7.0.0",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-rate-limit": "^5.1.3",
    "@types/express-session": "^1.18.2",
    "@types/node": "^20.4.5",
    "@types/pg": "^8.10.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  }
}
